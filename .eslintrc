{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "extends": ["prettier", "google"],
    "rules": {
        "semi": "off",
        "eol-last": "off",
        "object-curly-spacing": "off",
        "linebreak-style": "off",
        "require-jsdoc": "off",
        "indent": "off",
        "comma-dangle": "off",
        "space-before-blocks": "off",
        "no-tabs": "off",
        "space-before-function-paren": "off",
        "operator-linebreak": "off"
    },
    "plugins": [
      "prettier",
      "jest"
    ]
}